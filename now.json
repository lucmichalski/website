{
  "version": 2,
  "name": "grimp",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ],
  "env": {
    "HUBSPOT_API_KEY": "@hubspot_api_key"
  }
}