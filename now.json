{
  "name": "grimp",
  "builds": [
    {
      "src": "frontend/next.config.js",
      "use": "@now/next",
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1",
      "continue": true
    }
  ],
  "build": {
    "env": {
      "HUBSPOT_API_KEY": "@hubspot_api_key"
    }
  }
}